<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AjoutManuelSearch">

    <TextView
        android:id="@+id/textView9"
        android:layout_width="127dp"
        android:layout_height="32dp"
        android:layout_marginTop="20dp"
        android:fontFamily="@font/quicksand_bold"
        android:gravity="center"
        android:text="Medicalink"
        android:textColor="#000000"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="283dp"
        android:layout_height="28dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="59dp"
        android:fontFamily="@font/quicksand_bold"
        android:gravity="start"
        android:text="Ajouter un traitement"
        android:textColor="#000000"
        android:textSize="18sp"
        android:textStyle="normal"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView9" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/intervalle_regulier_button"
        android:layout_width="0dp"
        android:layout_height="73dp"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="20dp"
        android:background="@drawable/rounded_white_button_blue_stroke_background"
        android:fontFamily="@font/quicksand_bold"

        android:text="Intervalle rÃ©gulier"
        android:textAllCaps="false"
        android:textColor="#3F4791"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="@+id/retour_schema_prise2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textView10"
        app:layout_constraintTop_toBottomOf="@+id/debutAjd" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/debutAjd"
        android:layout_width="0dp"
        android:layout_height="73dp"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="20dp"
        android:background="@drawable/rounded_blue_button_blue_stroke_background"
        android:fontFamily="@font/quicksand_bold"

        android:text="Quotidiennement"
        android:textAllCaps="false"
        android:textColor="#3F4791"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="@+id/retour_schema_prise2"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/textView10"
        app:layout_constraintTop_toBottomOf="@+id/textView10" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/au_besoin_button"
        android:layout_width="0dp"
        android:layout_height="73dp"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="17dp"
        android:background="@drawable/rounded_white_button_blue_stroke_background"
        android:fontFamily="@font/quicksand_bold"

        android:text="Au besoin"
        android:textAllCaps="false"
        android:textColor="#3F4791"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="@+id/retour_schema_prise2"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/textView10"
        app:layout_constraintTop_toBottomOf="@+id/intervalle_regulier_button" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="29dp"
        android:layout_height="29dp"
        android:layout_marginStart="25dp"
        app:layout_constraintBottom_toBottomOf="@+id/textView9"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/cloche" />

    <ImageView
        android:id="@+id/retour_schema_prise2"
        android:layout_width="27dp"
        android:layout_height="27dp"
        android:layout_marginEnd="25dp"
        app:layout_constraintBottom_toBottomOf="@+id/textView10"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView10"
        app:srcCompat="@drawable/annuler" />

</androidx.constraintlayout.widget.ConstraintLayout>